<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jtalks.jcommune.model.entity">
    <class name="UserContactType" table="CONTACT_TYPE">
        <cache usage="nonstrict-read-write"/>
        <id column="TYPE_ID" name="id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <property name="uuid" column="UUID" unique="true" not-null="true"/>
        <property name="typeName" column="TYPE"/>
        <property name="icon" column="ICON"/>
        <property name="mask" column="MASK" length="50" />
        <property name="displayPattern" column="DISPLAY_PATTERN" length="255"/>

    </class>
</hibernate-mapping>
